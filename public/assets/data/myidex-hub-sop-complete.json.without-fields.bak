              {
                "name": "Hold",
                "color": "red",
                "description": "Indicates that the order is currently on hold. All In Progress activities are highlighted in red"
              },
              {
                "name": "Pending",
                "color": "gray",
                "description": "Indicates that the activities are yet to begin"
              },
              {
                "name": "In Progress",
                "color": "yellow",
                "description": "Indicates that activities are currently ongoing"
              },
              {
                "name": "Delayed In Progress",
                "color": "orange",
                "description": "Indicates that the activity has passed its estimated completion date"

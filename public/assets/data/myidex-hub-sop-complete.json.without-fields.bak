          },
          "activityStage": {
            "description": "Order's Activity Stage section displays the OMS ID and different activity tabs",
            "features": [
              "OMS ID is displayed in the top-left corner after the first activity submission",
              "Top slide bar displays Tab names of all activities with their current status",
              "Each Tab represents a different activity stage configured in Custom Form Master"
            ],
            "buttons": [
              {
                "name": "Save Data",
                "description": "Used to save the currently filled data without submitting"
              },
              {
                "name": "Skip Activity",
                "description": "Used to skip the current activity and move to the next one"
              },
              {
                "name": "Partial Submit",
                "description": "Used to partially submit the activity when not fully completed"
              },
              {
                "name": "Submit Without External Notification",
                "description": "Submit activity without sending notifications to external users; only internal team members receive notification"
              },
              {
                "name": "Final Submit",
                "description": "Final submission of the activity once all required information is completed"
              }
            ]
          },
          "actionButtons": {
            "location": "Top right corner",
            "actions": [
              {
                "name": "Send for Approval",
                "description": "Used when margin before order execution is less than required margin set by BLD person. Sends approval email to BLD person",
                "workflow": "BLD person reviews and approves the margin"
              },
              {
                "name": "Scope Change",
                "description": "Used to revert the workflow back to first activity",
                "workflow": "System resets all following activities, setting first one to In-Progress but keeps all data saved. Email notification sent to all internal stakeholders"
              },
              {
                "name": "Order Detail Update",
                "description": "Used to update details of an already submitted activity",
                "workflow": "Sends email to all internal users involved in the order"
              },
              {
                "name": "Project Hold",
                "description": "Used to pause or hold the order for any reason",
                "workflow": "Triggers email notification to all internal users"
              },
              {
                "name": "Project Hold Release",
                "description": "Used to resume a project that was previously on hold",
                "workflow": "Order continues from where it was paused. Sends email notification to all internal stakeholders"
              },
              {
                "name": "Reject Activity",
                "description": "Used when a mistake is found in a previously submitted activity",
                "workflow": "Activity is rejected with reason. Responsible user receives email about rejection"
              },
              {
                "name": "Partial Submit - Completed",
                "description": "Used when a partially submitted activity is now fully completed",
                "workflow": "Marks the activity as fully submitted"
              },
              {
                "name": "Delivered Status",
                "description": "Used after dispatch to confirm delivery",
                "workflow": "Displays all dispatched line items, including fields for date and attachments"
              }
